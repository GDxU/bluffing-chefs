{
  "name": "bluffing-chefs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:front": "parcel watch src/index.html --out-dir dist/public",
    "watch:server": "tsc src/server/server.ts --esModuleInterop --target ES5 --outDir dist --watch ",
    "watch": "concurrently \"npm run watch:front\" \"npm run watch:server\"",
    "dev": "concurrently --kill-others \"npm run watch\" \"nodemon dist/server/server.js\"",
    "build:front": "parcel build src/index.html --out-dir dist/public",
    "build:server": "tsc src/server/server.ts --esModuleInterop --target ES5 --outDir dist",
    "build": "npm run build:front && npm run build:server",
    "test:front": "jest",
    "start": "npm run build && node dist/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Voycawojka/bluffing-chefs.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/Voycawojka/bluffing-chefs/issues"
  },
  "homepage": "https://github.com/Voycawojka/bluffing-chefs#readme",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "concurrently": "^5.1.0",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.3",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  }
}
